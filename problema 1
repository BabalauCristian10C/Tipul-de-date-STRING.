string = str(input("Introduceti o propozitie "))
special = ["!", "@", "#", "$","%","^","&","*","(",")","=","-",",",".","/"] #etc....
cifre = ["1","0","2","3","4","5","6","7","8","9"]
print("Litere majuscule: ", sum(1 for c in string if c.isupper()))
print("Caractere speciale: ", sum(1 for c in string if c in special))
print("Numere: ", sum(1 for c in string if c in cifre))
